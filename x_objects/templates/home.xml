		<script src="[var:webroot][var:directory_name]/js/jquery.xml2json.js"/>
		<script src="[var:webroot][var:directory_name]/js/jquery-ui-1.8.12.custom.min.js"/>
		<script src="[var:webroot][var:directory_name]/js/xo-base.js"/>
		<script src="[var:webroot]js/[var:app_name].js"/>	
		<script>
			$.[var:app_name].init();
		</script>
