<?xml version="1.0" encoding="UTF-8"?>
<home doctype="strict">
	<head>
		<title>Test Home Page | [var:app_name]</title>	
		<!-- html5 reset default css include -->
		<css>[var:webroot][var:directory_name]/css/html5reset.css</css>
		<!-- x-objects css library -->
		<css>[var:webroot][var:directory_name]/css/x-objects.css</css>
		<css>[var:webroot]css/[var:app_name].css</css>	
		<script src="[var:webroot][var:directory_name]/js/jquery-1.6.3.js"/>
